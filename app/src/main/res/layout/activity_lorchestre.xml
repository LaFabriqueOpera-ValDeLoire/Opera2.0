<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Lorchestre"
    android:background="@drawable/fovdl_appli_orchestre">

    <TextView

        android:id="@+id/textViewOrchestre"
        android:layout_width="235dp"

        android:layout_height="57dp"
        android:gravity="center_horizontal"
        android:text="l'orchestre"
        android:textAllCaps="true"
        android:textColor="@color/black"
        android:textSize="90px"

        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.3" />

    <TextView
        android:id="@+id/cordes"
        android:layout_width="75dp"
        android:layout_height="105dp"

        android:alpha="0"
        android:gravity="center_horizontal"
        android:text="cordes"
        android:textAllCaps="true"
        android:textColor="@color/black"
        android:textSize="40px"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.6" />

    <TextView
        android:id="@+id/ventbois"
        android:layout_width="112dp"
        android:layout_height="93dp"

        android:alpha="0"
        android:gravity="center_horizontal"
        android:text="vent bois"
        android:textAllCaps="true"
        android:textColor="@color/black"
        android:textSize="40px"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.77" />

    <TextView
        android:id="@+id/cuivres"
        android:layout_width="89dp"
        android:layout_height="115dp"

        android:alpha="0"
        android:gravity="center_horizontal"
        android:text="cuivres"
        android:textAllCaps="true"
        android:textColor="@color/grey"
        android:textSize="40px"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.69"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.78" />

    <TextView
        android:id="@+id/claviers"
        android:layout_width="81dp"
        android:layout_height="113dp"

        android:alpha="0"
        android:gravity="center_horizontal"
        android:text="claviers"
        android:textAllCaps="true"
        android:textColor="@color/grey"
        android:textSize="40px"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.96"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.63" />

    <TextView
        android:id="@+id/percussions"
        android:layout_width="245dp"
        android:layout_height="43dp"

        android:alpha="0"
        android:gravity="center_horizontal"
        android:text="percussions"
        android:textAllCaps="true"
        android:textColor="@color/grey"
        android:textSize="40px"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.90999997" />

    <ImageView
        android:id="@+id/backtohomepage"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/fovdl_appli_boutonretour" />


</androidx.constraintlayout.widget.ConstraintLayout>